    <section id="header" class="navbar_bs flex flex-1 flex-row mt-[42px] justify-center lg:gap-x-60 md:gap-x-[100px] ">
        <img class="logo_bs md:scale-[0.6] lg:scale-[1]" src="/src/logo.svg" />
        <div class="links_bs flex items-center lg:text-[20px] md:text-[15px] gap-[3rem] text-white font-[Montserrat] font-[500]">
          <a class="hover:text-[#85ec68]" href="#hero ">Home</a>
          <a class="hover:text-[#85ec68]" href="#Features">Features</a>
          <a class="hover:text-[#85ec68]" href="#Pricing">Pricing</a>
        </div>
        <button class="bt login border rounded-lg lg:text-[12px] md:text-[8px] font-[800] border-white lg:px-[47px] md:px-[28px] text-white font-[Inter] hover:bg-[#85ec68] hover:text-[#002226] hover:border-transparent " >LOGIN</button>
    </section>
    <section id="hero">
      <div class="flex justify-center flex-col items-center mt-[50px] space-y-0">
        <div class="flex justify-center relative top-10">
          <div class="in txt text-white lg:text-[8rem] md:text-[5rem] font-black font-[Montserrat] flex relative z-10">
